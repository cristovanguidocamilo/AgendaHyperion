program AgendaHyperion;

uses
  Vcl.Forms,
  untPrincipal in 'untPrincipal.pas' {Form1},
  untCadastro in 'untCadastro.pas' {Form2};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
